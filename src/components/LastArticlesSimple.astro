---
const response = await fetch('https://adriencaubel.substack.com/api/v1/posts/?limit=5', {
    headers: {
      'Content-Type': 'application/json',
    }});
console.log(response)
const articles = await response.json();
---
<section class="text-gray-600">
    {articles.map((article: { canonical_url: string | URL | null | undefined; postTags: { name: unknown; }[]; title: unknown; }) => (
        <a href={article.canonical_url} target="_blank">
            <div class="flex items-baseline border-b-2 py-3">
                <div>
                    <span class="uppercase">{article.postTags[0].name}</span><br>
                    <span>{article.title}</span>
                </div>
            </div> 
        </a>
    ))}
</section>